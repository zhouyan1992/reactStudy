#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Volumes/Macintosh HD - 数据/study/reactStudy/node_modules/.pnpm/@commitlint+cli@18.4.3_typescript@5.3.3/node_modules/@commitlint/cli/node_modules:/Volumes/Macintosh HD - 数据/study/reactStudy/node_modules/.pnpm/@commitlint+cli@18.4.3_typescript@5.3.3/node_modules/@commitlint/node_modules:/Volumes/Macintosh HD - 数据/study/reactStudy/node_modules/.pnpm/@commitlint+cli@18.4.3_typescript@5.3.3/node_modules:/Volumes/Macintosh HD - 数据/study/reactStudy/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Volumes/Macintosh HD - 数据/study/reactStudy/node_modules/.pnpm/@commitlint+cli@18.4.3_typescript@5.3.3/node_modules/@commitlint/cli/node_modules:/Volumes/Macintosh HD - 数据/study/reactStudy/node_modules/.pnpm/@commitlint+cli@18.4.3_typescript@5.3.3/node_modules/@commitlint/node_modules:/Volumes/Macintosh HD - 数据/study/reactStudy/node_modules/.pnpm/@commitlint+cli@18.4.3_typescript@5.3.3/node_modules:/Volumes/Macintosh HD - 数据/study/reactStudy/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../cli.js" "$@"
else
  exec node  "$basedir/../../cli.js" "$@"
fi
